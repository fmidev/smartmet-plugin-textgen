
database_servers:
{
	# MySQL dictionary database definitions
	mysql_dictionary:
	{
		host		= "mysql_host";
		username	= "mysql_user_name";
		password	= "mysql_password";
		database	= "mysql_database_name";
	};

	# PostGIS database definitions
	postgis:
	(
		{
			host		= "postgis_default_host";
			port    	= "postgis_default_port";				
			database	= "postgis_default_database_name";
			username	= "postgis_default_user_name";
			password	= "postgis_default_password";
			client_encoding	= "postgis_default_client_encoding";
		}
	);
}
